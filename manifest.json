{
    "name" : "AdBlocker",
    "description" : "Extension for blocking advertisements and trackers",
    "version" : "1.0.0",
    "manifest_version" : 3,
    "permissions" : [
        "declarativeNetRequest"
    ],
    "declarative_net_request" : {
        "rule_resources" : [
            {
                "id" : "ruleset_1",
                "enabled" : true,
                "path" : "rules.json"
            }
        ]
    },
    "action": {
        "default_icon": "./Images/shield.png"
    }
}